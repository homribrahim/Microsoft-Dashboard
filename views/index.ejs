<!doctype html>
<html lang="en">

<head>

	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<title>Dashboard</title>

	<!-- Bootstrap CSS-->
	<link rel="stylesheet" href="../views/assets/modules/bootstrap-5.1.3/css/bootstrap.css">
	<!-- Style CSS -->
	<link rel="stylesheet" href="../views/assets/css/style.css">
	<!-- FontAwesome CSS-->
	<link rel="stylesheet" href="../views/assets/modules/fontawesome6.1.1/css/all.css">
	<!-- Boxicons CSS-->
	<link rel="stylesheet" href="../views/assets/modules/boxicons/css/boxicons.min.css">
	<!-- ICON -->
	<link rel="icon" type="icon" href="../Images/logo_no.png">
</head>

<body>

	<!--Topbar -->
	<div class="topbar transition">
		<div class="bars">
			<button type="button" class="btn transition" id="sidebar-toggle">
				<i class="fa fa-bars"></i>
			</button>
		</div>

		<div class="menu">

			<h5>
				Welcome , <%=user.charAt(0).toUpperCase() + user.slice(1) %>
			</h5>

			<ul>
				<li class="nav-item dropdown">
					<a class="nav-link" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown"
						aria-expanded="false">
						<img src="../views/assets/images/avatar/avatar-1.png" alt="">
					</a>
					<ul class="dropdown-menu" aria-labelledby="navbarDropdown">
						<a class="dropdown-item" href="/users/edit_profile"><i class="fa fa-cog size-icon-1"></i>
							<span>Paramètres</span></a>
						<hr class="dropdown-divider">
						<a class="dropdown-item" href="#"><i class="fa fa-sign-out-alt  size-icon-1"></i> <span>Se
								Déconnecter</span></a>
					</ul>
				</li>
			</ul>
		</div>
	</div>

	<!--Sidebar-->
	<div class="sidebar transition overlay-scrollbars animate__animated  animate__slideInLeft">
		<div class="sidebar-content">
			<div id="sidebar">

				<!-- Logo -->
				<div class="logo">
					<center>
						<img src="../Images/logo_no.png" style="width: 100px; height: 100px ;margin-top:20px;">
					</center>
				</div>

				<ul class="side-menu">
					<li>
						<a href="/users/index" class="active">
							<i class='bx bxs-dashboard icon'></i> Stats
						</a>
					</li>

					<!-- Divider-->
					<li class="divider" data-text="STARTER">Actualités</li>

					<li>
						<a href="/users/home">
							<i class='bx bxs-home icon'></i>
							Home
						</a>
					</li>

					<!-- Divider-->
					<li class="divider" data-text="Atrana">Extra</li>

					<li>
						<a href="/users/fiche">
							<i class='bx bxs-notepad icon'></i>
							Assisting Sheet
						</a>


					<li>
						<a href="#">
							<i class='bx bx-user icon'></i>
							Profile
							<i class='bx bx-chevron-right icon-right'></i>
						</a>
						<ul class="side-dropdown">
							<li><a href="/users/edit_profile">Edit Profile</a></li>
							<li><a href="/users/des_profile">Disable Account</a></li>

						</ul>
					</li>

					<li>
						<a href="/users/help">
							<i class='bx bxs-hand icon'></i>
							Help
						</a>
					</li>

					<li>
						<a href="#" style="color: #ff8080;">
							<i class='bx bxs-log-out icon'></i>
							Sign Out
						</a>

					</li>

				</ul>

				<div class="ads">
					<div class="wrapper">
						<div class="help-icon"><i class="fa fa-circle-question fa-3x"></i></div>
						<p>Need Help With <strong>MICROSOFT</strong>?</p>
						<a href="/users/contact" class="btn-upgrade">Contact Us</a>
					</div>
				</div>
			</div>

		</div>
	</div>
	</div><!-- End Sidebar-->


	<div class="sidebar-overlay"></div>


	<!--Content Start-->
	<div class="content-start transition">
		<div class="container-fluid dashboard">
			<div class="content-header">
				<h1>Stats</h1>
				<p></p>
			</div>

			<div class="row">

				<div class="col-md-6 col-lg-3">
					<div class="card">
						<div class="card-body">
							<div class="row">
								<div class="col-4 d-flex align-items-center">
									<i class="fas fa-inbox icon-home bg-primary text-light"></i>
								</div>
								<div class="col-8">
									<p>
										Current
										<br>
										month income
									</p>
									<h5>
										<%= rev %>
									</h5>
									<h5>Dinars</h5>
								</div>
							</div>
						</div>
					</div>
				</div>

				<div class="col-md-6 col-lg-3">
					<div class="card">
						<div class="card-body">
							<div class="row">
								<div class="col-4 d-flex align-items-center">
									<i class="fas fa-clipboard-list icon-home bg-success text-light"></i>
								</div>
								<div class="col-8">
									<p>Time Spent (H)</p>
									<h5>
										<%= heure %> Hours
									</h5>
								</div>
							</div>
						</div>
					</div>
				</div>

				<div class="col-md-6 col-lg-3">
					<div class="card">
						<div class="card-body">
							<div class="row">
								<div class="col-4 d-flex align-items-center">
									<i class="fas fa-id-card  icon-home bg-warning text-light"></i>
								</div>
								<div class="col-8">
									<p>Members Department Machine Learning MICROSOFT</p>
									<h5>
										<%= employe_dep %>
									</h5>
								</div>
							</div>
						</div>
					</div>
				</div>

				<div class="col-md-6 col-lg-3">
					<div class="card">
						<div class="card-body">
							<div class="row">
								<div class="col-4 d-flex align-items-center">
									<i class="fas fa-id-card  icon-home bg-danger text-light"></i>
								</div>
								<div class="col-8">
									<p>Total <br> Members <br> MICROSOFT</p>
									<h5>
										<%= employe_tot %>
									</h5>
								</div>
							</div>
						</div>
					</div>

				</div>

				<div class="col-md-6">
					<div class="card">
						<div class="card-header">
							<h2>Current Points</h2>
							<br>
							<br>
							<br>
						</div>
						<div class="card-body">
							<center>
								<h1 style="font-size: 5rem ;color: green;">
									<%= pts %> Points
								</h1>
							</center>
							<br>
							<br>
							<br>
							<br>
						</div>
					</div>
				</div>

				<div class="col-md-6">
					<div class="card">
						<div class="card-header">
							<h4>Ranking</h4>
						</div>
						<div class="card-body pb-4">
							<ul>
								<li>
									<h3 style="color: green;">
										<%= tab %>

									</h3>
								</li>
							</ul>
							<!-- <div class="recent-message d-flex px-4 py-3">
								<div class="avatar avatar-lg">
									<img src="../Icons/user.png">
								</div>
								<div class="name ms-4">
									<h5 class="mb-1">Brahim Homri <span style="color: black;">(1)</span></h5>
									<h6 class="text-muted mb-0">Informatique</h6>
								</div>
							</div> -->
							<!-- <div class="recent-message d-flex px-4 py-3">
								<div class="avatar avatar-lg">
									<img src="../Icons/user.png">
								</div>
								<div class="name ms-4">
									<h5 class="mb-1">Eya Belghith <span style="color: black;">(2)</span></h5>
									<h6 class="text-muted mb-0">Informatique</h6>
								</div>
							</div> -->
							<!-- <div class="recent-message d-flex px-4 py-3">
								<div class="avatar avatar-lg">
									<img src="../Icons/user.png">
								</div>
								<div class="name ms-4">
									<h5 class="mb-1">Bill Gates <span style="color: black;">(3)</span></h5>
									<h6 class="text-muted mb-0">Informatique</h6>
								</div>
							</div> -->
							<!-- <div class="px-4">
								<button class='btn btn-block btn-xl btn-primary font-bold mt-3'>Voir
									Classement Total</button>
							</div> -->
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>


	<!-- Footer -->
	<footer>
		<div class="footer">
			<div class="float-start">
				<p>2022 &copy; CIMB</p>
			</div>
			<div class="float-end">
				<p>Crafted with
					<span class="text-danger">
						<i class="fa fa-heart"></i> by
						<a href="https://www.linkedin.com/in/brahim-homri-30a7a9199/" class="author-footer">&nbsp;
							Brahim</a>
					</span>
				</p>
			</div>
		</div>
	</footer>


	<!-- Preloader -->
	<div class="loader">
		<div class="spinner-border text-light" role="status">
			<span class="sr-only">Loading...</span>
		</div>
	</div>

	<!-- Loader -->
	<div class="loader-overlay"></div>

	<!-- JS Libraies -->
	<script src="../views/assets/modules/jquery/jquery.min.js"></script>
	<script src="../views/assets/modules/bootstrap-5.1.3/js/bootstrap.bundle.min.js"></script>
	<script src="../views/assets/modules/popper/popper.min.js"></script>

	<!-- Template JS File -->
	<script src="../views/assets/js/script.js"></script>

</body>

</html>